{
  "manifest_version": 2,
  "name": "Pomodoro Timer",
  "version": "1.0",
  "description": "Simple Pomodoro timer extension",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "pomodoro.png",
    "default_popup": "popup.html",
    "default_title": "Pomodoro Timer"
  },
  "icons": {
    "48": "pomodoro_48.png",
    "128": "pomodoro_128.png"
  },
  "permissions": ["activeTab"]
}
